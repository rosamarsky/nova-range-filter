{
    "name": "rosamarsky/nova-range-filter",
    "version": "1.0",
    "description": "A Laravel Nova range filter.",
    "keywords": [
        "laravel",
        "nova",
        "range",
        "filter",
        "slider"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rosamarsky\\RangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rosamarsky\\RangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
